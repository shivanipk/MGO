<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.mgo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AfterLogin">
            <intent-filter>
                <action android:name="com.example.administrator.mgo.AfterLogin" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".AddQRManually">
            <intent-filter>
                <action android:name="com.example.administrator.mgo.AddQRManually" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


        <activity android:name=".ViewInfo">
            <intent-filter>
                <action android:name="com.example.administrator.mgo.ViewInfo" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


            </application>

</manifest>